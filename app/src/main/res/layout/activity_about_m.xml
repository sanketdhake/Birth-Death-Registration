                    android:textSize="20dp" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/r5"
                android:layout_below="@+id/r4"
                android:layout_marginTop="150dp"
                android:layout_marginBottom="150dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorBack">

                <ImageView
                    android:id="@+id/imgs"
                    android:layout_width="350dp"
                    android:layout_height="250dp"
                    android:layout_centerHorizontal="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="100dp"
                    app:srcCompat="@drawable/ic_user" />

                <TextView
                    android:id="@+id/tvh1s"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imgs"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="372dp"
                    android:fontFamily="sans-serif-medium"
                    android:text="Sanket Dhake"
                    android:textColor="@color/white"
                    android:textSize="25dp" />

                <TextView
                    android:id="@+id/tvh2s"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tvh1s"
                    android:layout_centerHorizontal="true"
                    android:fontFamily="sans-serif-medium"
                    android:text="Group Member"
                    android:textColor="@color/white"
                    android:textSize="25dp" />

                <TextView
                    android:id="@+id/tvds"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tvh2s"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="20dp"
                    android:textColor="@color/editTextBG"
                    android:fontFamily="sans-serif-light"
                    android:text="1606028"
                    android:textSize="20dp" />

            </RelativeLayout>


        </RelativeLayout>

    </ScrollView>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nview35"
        app:headerLayout="@layout/header"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@color/white"
        app:itemTextColor="@color/darkgray"
        app:itemIconTint="@color/darkgray"
        app:menu="@menu/drawermenum"
        android:layout_gravity="start">

    </com.google.android.material.navigation.NavigationView>

</androidx.drawerlayout.widget.DrawerLayout>